{
  "version": 2,
  "builds": [
    { "src": "public/index.php", "use": "@vercel/php" },
    {
      "src": "public/build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|map))", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "public/index.php" }
  ]
}
